{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 20
    },
    "description": {
      "type": "richtext",
      "required": true,
      "minLength": 3
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female ",
        "Other"
      ],
      "default": "Other"
    },
    "Available": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "calendly": {
      "type": "string",
      "required": true
    },
    "pdf": {
      "type": "string",
      "required": true
    },
    "linkedIn": {
      "type": "string",
      "required": true
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "students"
    },
    "majors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::major.major",
      "inversedBy": "students"
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "inversedBy": "students"
    },
    "favorite_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "favorites"
    },
    "portfolioLink": {
      "type": "text"
    }
  }
}
